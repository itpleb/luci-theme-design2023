#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/op-dev/luci-theme-vdesign/node_modules/.pnpm/ts-node@10.7.0_@types+node@17.0.25_typescript@4.6.3/node_modules/ts-node/dist/node_modules:/home/dev/op-dev/luci-theme-vdesign/node_modules/.pnpm/ts-node@10.7.0_@types+node@17.0.25_typescript@4.6.3/node_modules/ts-node/node_modules:/home/dev/op-dev/luci-theme-vdesign/node_modules/.pnpm/ts-node@10.7.0_@types+node@17.0.25_typescript@4.6.3/node_modules:/home/dev/op-dev/luci-theme-vdesign/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dev/op-dev/luci-theme-vdesign/node_modules/.pnpm/ts-node@10.7.0_@types+node@17.0.25_typescript@4.6.3/node_modules/ts-node/dist/node_modules:/home/dev/op-dev/luci-theme-vdesign/node_modules/.pnpm/ts-node@10.7.0_@types+node@17.0.25_typescript@4.6.3/node_modules/ts-node/node_modules:/home/dev/op-dev/luci-theme-vdesign/node_modules/.pnpm/ts-node@10.7.0_@types+node@17.0.25_typescript@4.6.3/node_modules:/home/dev/op-dev/luci-theme-vdesign/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
fi
